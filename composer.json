{
    "name": "nanjingboy/assets",
    "type": "library",
    "description": "Assets Manager for PHP",
    "keywords": ["uglify", "javaScript", "coffeeScript", "css", "scss", "less"],
    "homepage": "https://github.com/nanjingboy/assets",
    "license": "MIT",
    "authors": [{
        "name": "Tom Huang",
        "email": "hzlhu.dargon@gmail.com"
    }],
    "require": {
        "php": ">=5.4.0",
        "nanjingboy/assetloader": "*",
        "nanjingboy/commander": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "bin": ["cli/assets"],
    "autoload": {
        "classmap": ["lib/", "cli/"],
        "files": ["assets.php"]
    }
}